class Human:
    def __init__(self,run,dance):
        self.run=run
        self.dance=dance
    def display(self):
         print("i will display")
    def displaying(self):
        print("i'm displaying....")
class student(Human):
    def __init__(self,read,write):
        self.read=read 
        self.write=write
        print("i'm here for studying")
    def display(self):
        print("display from human1 ")
        super().display()
