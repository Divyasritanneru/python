def valid(s):
    while(True):
        if '[]' in s:
            s=s.replace('[]','')
        elif '()' in s:
            s=s.replace('()','')
        elif '{}' in s:
            s=s.replace('{}','')
        else:
           return not s    
s=input()
if valid(s):
    print('YES')
else:
    print('NO')
